<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h1>Animals</h1>
  <% if (error) { %>
    <h2><% error %></h2>
  <% } %>
  <ul>
    <% animals.forEach(function(animal) { %>
      <li>
        <%= animal.text %>
        - 
        <%= animal.local ? 'local': 'not local' %> 
      </li>
    <% }) %>
  </ul>
</body>
</html>